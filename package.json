{
  "name": "peoplepower-node",
  "version": "1.0.0",
  "description": "People Power Node Scripts",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@react-pdf/renderer": "^1.6.8",
    "airlock-server": "^0.2.8",
    "airtable": "^0.8.1",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "highcharts-export-server": "^2.0.24",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.1",
    "tslib": "^1.11.1"
  },
  "scripts": {
    "add-require-to-airtable": "(echo \"require('dotenv-safe').config();\" && cat airtable/airtable.js) > temp.js && mv temp.js airtable/airtable.js",
    "generate-schema": "generate-airtable-schema && pretty-quick && yarn add-require-to-airtable",
    "start": "babel-node index.js"
  },
  "devDependencies": {
    "airtable-schema-generator": "^1.3.4",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "1.18.2",
    "pretty-quick": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint ./ --fix",
      "git add"
    ]
  },
  "airtable-schema-generator": {
    "output": "airtable",
    "mode": "auto-headless"
  }
}
